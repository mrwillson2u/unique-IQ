{
  "manifest_version": 2,
  "name": "Unique IQ Scanner",
  "short_name": "Unique IQ",
  "description": "This extension learns about you, your interests, and how you learn to be able to curate content to give back to you.",
  "version": "1.5",
  "key": "",
  "background": { "scripts": ["bower_components/firebase/firebase.js", "uniqueIq.js"] },
  "browser_action": {
      "name": "Click to change the icon",
      "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' https://*.firebaseio.com; object-src 'self'; style-src 'self' 'unsafe-inline'",
  "oauth2": {
    "client_id": "302085066950-j0v4n97jngv19all32l4l9jupcr0ac0s.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/drive"
    ]
  },

  "permissions": [
    "activeTab",
    "tabs",
    "history",
    "identity",
    "identity.email"
  ]
}
